<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/plant-metabolite-ontology#"
     xml:base="http://example.org/plant-metabolite-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://example.org/plant-metabolite-ontology"/>
    
    <!-- Sample anatomical terms for testing -->
    <owl:Class rdf:about="http://example.org/plant-metabolite-ontology#PlantPart">
        <rdfs:label>Plant Part</rdfs:label>
        <rdfs:comment>A part of a plant organism</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/plant-metabolite-ontology#Leaf">
        <rdfs:subClassOf rdf:resource="http://example.org/plant-metabolite-ontology#PlantPart"/>
        <rdfs:label>Leaf</rdfs:label>
        <rdfs:comment>A flattened structure of a higher plant, typically green and blade-like</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/plant-metabolite-ontology#Root">
        <rdfs:subClassOf rdf:resource="http://example.org/plant-metabolite-ontology#PlantPart"/>
        <rdfs:label>Root</rdfs:label>
        <rdfs:comment>The part of a plant which typically lies below the surface of the soil</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/plant-metabolite-ontology#Stem">
        <rdfs:subClassOf rdf:resource="http://example.org/plant-metabolite-ontology#PlantPart"/>
        <rdfs:label>Stem</rdfs:label>
        <rdfs:comment>The main trunk of a plant</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/plant-metabolite-ontology#Flower">
        <rdfs:subClassOf rdf:resource="http://example.org/plant-metabolite-ontology#PlantPart"/>
        <rdfs:label>Flower</rdfs:label>
        <rdfs:comment>The reproductive structure found in flowering plants</rdfs:comment>
    </owl:Class>
    
    <!-- Sample metabolite classes -->
    <owl:Class rdf:about="http://example.org/plant-metabolite-ontology#Metabolite">
        <rdfs:label>Metabolite</rdfs:label>
        <rdfs:comment>A small molecule that is an intermediate or end product of metabolism</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/plant-metabolite-ontology#SecondaryMetabolite">
        <rdfs:subClassOf rdf:resource="http://example.org/plant-metabolite-ontology#Metabolite"/>
        <rdfs:label>Secondary Metabolite</rdfs:label>
        <rdfs:comment>A metabolite that is not directly involved in normal growth, development, or reproduction</rdfs:comment>
    </owl:Class>
    
    <!-- Sample relationship properties -->
    <owl:ObjectProperty rdf:about="http://example.org/plant-metabolite-ontology#accumulates_in">
        <rdfs:label>accumulates in</rdfs:label>
        <rdfs:comment>Indicates that a metabolite accumulates in a specific plant part</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/plant-metabolite-ontology#Metabolite"/>
        <rdfs:range rdf:resource="http://example.org/plant-metabolite-ontology#PlantPart"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/plant-metabolite-ontology#made_via">
        <rdfs:label>made via</rdfs:label>
        <rdfs:comment>Indicates the biosynthetic pathway through which a metabolite is produced</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/plant-metabolite-ontology#affects">
        <rdfs:label>affects</rdfs:label>
        <rdfs:comment>Indicates a functional relationship between entities</rdfs:comment>
    </owl:ObjectProperty>
</rdf:RDF>
